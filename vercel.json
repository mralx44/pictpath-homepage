{
  "trailingSlash": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/privacy.html",
      "destination": "/privacy/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/privacy/",
      "destination": "/privacy.html"
    },
    {
      "source": "/delete-data/",
      "destination": "/delete-data.html"
    },
    {
      "source": "/delete-data",
      "destination": "/delete-data.html"
    },
    {
      "source": "/hiking/",
      "destination": "/hiking.html"
    },
    {
      "source": "/hiking",
      "destination": "/hiking.html"
    },
    {
      "source": "/road-trips/",
      "destination": "/road-trips.html"
    },
    {
      "source": "/road-trips",
      "destination": "/road-trips.html"
    },
    {
      "source": "/biking/",
      "destination": "/biking.html"
    },
    {
      "source": "/biking",
      "destination": "/biking.html"
    },
    {
      "source": "/events/",
      "destination": "/events.html"
    },
    {
      "source": "/events",
      "destination": "/events.html"
    }
  ]
}
